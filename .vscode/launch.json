{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug(node2/launch)",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/ava/profile.js",
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [

            ],

            "args": [
                "--verbose",
                "${workspaceRoot}/bin/test/${command.extension.mock-debug.getProgramName}"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "smartStep": true,
            "sourceMaps": true,
            "internalConsoleOptions": "openOnSessionStart",
            "stopOnEntry": false
        }
    ]
}